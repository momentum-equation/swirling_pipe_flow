functions
{
	probes2
	{
		type probes;
		functionObjectLibs ("libsampling.so");
		//dictionary probesDict;
		probeLocations
			(
				(0 0.0095 0)
				(0 0.0105 0)
				(0 0.0035 0)
			);
		fields 	(p U);
		outputControl       timeStep;
		outputInterval      1;
	}

}

