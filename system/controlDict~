FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

//application moveDynamicMesh;

application     pimpleDyMFoam;

startFrom       startTime;

//startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         0.12;

deltaT          0.000004;

writeControl    timeStep;

writeInterval   25;

purgeWrite     0;

writeFormat     binary;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

adjustTimeStep  no;

maxCo           0.2;

